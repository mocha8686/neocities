---
const links = [
	{ name: 'about', href: '/about/' },
	{ name: 'blog', href: '/blog/' },
	{ name: 'learning in public', href: '/learning/' },
	{ name: 'shrines', href: '/shrines/' },
	{ name: 'fun stuff', href: '/fun/' },
	{ name: 'guestbook', href: 'https://mocha8686.atabook.org/' },
];
---

<nav>
	<ul>
		{
			links.map(({ name, href }) => (
				<li>
					<a {href}>{name}</a>
				</li>
			))
		}
	</ul>
</nav>

<style>
	ul {
		line-height: 1.3;
	}
</style>
