---
import Section from '$/components/Section.astro';
import WebringsNoAI from '$/components/WebringsNoAI.astro';
import WebringsTransingTheInternet from '$/components/WebringsTransingTheInternet.astro';
---

<Section class="container" header="Webrings">
	<WebringsNoAI />
	<WebringsTransingTheInternet />
</Section>

<style>
	.container {
		display: grid;
		grid-template-columns: repeat(auto-fit, minmax(12rem, 1fr));
		grid-auto-rows: fit-content;
		gap: 2rem;
		place-items: center;
		padding-block: 2rem;
	}
</style>
